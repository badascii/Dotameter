
= link_to_next_page @matches, 'Next Page'
= paginate @matches.page(params[:page])

%ul.matches{"ng-controller" => "matchCtrl"}
  - @matches.each do |rails_match|
    %li{"ng-model" => "matches.#{rails_match.match_id}"}
      = link_to rails_match.match_id, match_path(rails_match.match_id)

/ %ul.matches{"ng-controller" => "matchCtrl"}
/   %li{"ng-repeat" => "match in matches"}
/     %a{"ng-href" => "/matches/{{ match.match_id}}"}
/       {{ match.match_id }}
